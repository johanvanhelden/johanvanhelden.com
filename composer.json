{
    "name": "johanvanhelden/johanvanhelden.com",
    "description": "My personal website.",
    "type": "project",
    "license": "gpl-3.0",
    "require": {
        "php": "^7.4",
        "dillingham/nova-attach-many": "^1.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.0",
        "laravel/nova": "^3.9",
        "laravel/ui": "^3.0",
        "league/commonmark": "^1.5",
        "livewire/livewire": "^2.2",
        "optimistdigital/nova-sortable": "^1.4",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^1.9",
        "spatie/laravel-activitylog": "^3.14",
        "spatie/laravel-permission": "^3.16",
        "spatie/laravel-queueable-action": "^2.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "johnkary/phpunit-speedtrap": "^3.1",
        "laravel/dusk": "^6.6",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.6",
        "phpunit/phpunit": "^9.3",
        "sensiolabs/security-checker": "^6.0",
        "spatie/laravel-web-tinker": "^1.7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk",
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish --ansi",
            "@php artisan telescope:publish --ansi",
            "@php artisan horizon:publish --ansi",
            "@php artisan vendor:publish --tag=web-tinker-assets --force --ansi",
            "@php ./vendor/sensiolabs/security-checker/security-checker security:check --ansi"
        ],
        "test": [
            "@php artisan migrate --env=testing --ansi",
            "@php artisan bootstrap:application --env=testing --ansi",
            "./vendor/bin/phpunit --testsuite Unit",
            "./vendor/bin/phpunit --testsuite Feature"
        ],
        "test-fresh": [
            "@php artisan migrate:fresh --env=testing --ansi",
            "@php artisan bootstrap:application --env=testing --ansi",
            "./vendor/bin/phpunit --testsuite Unit",
            "./vendor/bin/phpunit --testsuite Feature"
        ],
        "test-coverage": [
            "@php artisan migrate --env=testing --ansi",
            "@php artisan bootstrap:application --env=testing --ansi",
            "@php -d pcov.enabled=1 ./vendor/bin/phpunit --coverage-html public_html/tests-report"
        ],
        "test-browser": [
            "@php artisan migrate --env=testing --ansi",
            "@php artisan bootstrap:application --env=testing --ansi",
            "@php artisan dusk"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
