{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "anhskohbo/no-captcha": "^3.1",
        "dillingham/nova-attach-many": "^1.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "inertiajs/inertia-laravel": "^0.2",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^6.0",
        "laravel/horizon": "^3.0",
        "laravel/nova": "^2.0",
        "laravel/scout": "^7.0",
        "laravelcollective/html": "^6.0",
        "optimistdigital/nova-sortable": "^1.1",
        "owen-it/laravel-auditing": "^9.3",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^1.2",
        "spatie/eloquent-sortable": "^3.7",
        "spatie/laravel-permission": "^3.0",
        "tightenco/ziggy": "^0.8.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.3",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.8",
        "johnkary/phpunit-speedtrap": "^3.1",
        "laravel/dusk": "^5.5",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.5.0",
        "phpunit/phpunit": "^8.0",
        "sensiolabs/security-checker": "^6.0",
        "spatie/laravel-web-tinker": "^1.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk",
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish",
            "@php artisan telescope:publish",
            "@php artisan horizon:assets",
            "@php artisan vendor:publish --tag=web-tinker-assets --force",
            "@php ./vendor/sensiolabs/security-checker/security-checker security:check"
        ],
        "test": [
            "php artisan migrate --env=testing --ansi",
            "php artisan bootstrap:application --env=testing --ansi",
            "./vendor/bin/phpunit --testsuite Unit",
            "./vendor/bin/phpunit --testsuite Feature"
        ],
        "test-coverage": [
            "./vendor/bin/phpunit --coverage-html public_html/tests-report"
        ],
        "test-fresh": [
            "php artisan migrate:fresh --env=testing --ansi",
            "php artisan bootstrap:application --env=testing --ansi",
            "./vendor/bin/phpunit --testsuite Unit",
            "./vendor/bin/phpunit --testsuite Feature"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
