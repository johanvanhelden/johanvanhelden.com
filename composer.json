{
    "name": "johanvanhelden/johanvanhelden.com",
    "description": "My personal website.",
    "type": "project",
    "license": "gpl-3.0",
    "require": {
        "php": "^7.4",
        "anhskohbo/no-captcha": "^3.1",
        "dillingham/nova-attach-many": "^1.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "inertiajs/inertia-laravel": "^0.2",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^7.0",
        "laravel/horizon": "^4.3",
        "laravel/nova": "^3.6",
        "laravel/scout": "^8.0",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.0",
        "optimistdigital/nova-sortable": "^1.1",
        "owen-it/laravel-auditing": "^10.0",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^1.2",
        "spatie/laravel-permission": "^3.11",
        "spatie/laravel-queueable-action": "^2.6",
        "tightenco/ziggy": "^0.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.4",
        "beyondcode/laravel-query-detector": "^1.3",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "johnkary/phpunit-speedtrap": "^3.1",
        "laravel/dusk": "^6.0",
        "laravel/telescope": "^3.2",
        "laravel/tinker": "^2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.2",
        "nunomaduro/larastan": "^0.6",
        "phpunit/phpunit": "^9.0",
        "sensiolabs/security-checker": "^6.0",
        "spatie/laravel-web-tinker": "^1.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk",
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish",
            "@php artisan telescope:publish",
            "@php artisan horizon:publish",
            "@php artisan vendor:publish --tag=web-tinker-assets --force",
            "@php ./vendor/sensiolabs/security-checker/security-checker security:check"
        ],
        "test": [
            "@php -r \"file_exists('.env.testing') || copy('.env.testing.example', '.env.testing');\"",
            "@php artisan migrate --env=testing --ansi",
            "@php artisan bootstrap:application --env=testing --ansi",
            "./vendor/bin/phpunit --testsuite Unit",
            "./vendor/bin/phpunit --testsuite Feature"
        ],
        "test-fresh": [
            "@php -r \"file_exists('.env.testing') || copy('.env.testing.example', '.env.testing');\"",
            "@php artisan migrate:fresh --env=testing --ansi",
            "@php artisan bootstrap:application --env=testing --ansi",
            "./vendor/bin/phpunit --testsuite Unit",
            "./vendor/bin/phpunit --testsuite Feature"
        ],
        "test-coverage": [
            "@php -r \"file_exists('.env.testing') || copy('.env.testing.example', '.env.testing');\"",
            "@php artisan migrate --env=testing --ansi",
            "@php artisan bootstrap:application --env=testing --ansi",
            "@php -d pcov.enabled=1 ./vendor/bin/phpunit --coverage-html public_html/tests-report"
        ],
        "test-browser": [
            "@php -r \"file_exists('.env.dusk.local') || copy('.env.dusk.local.example', '.env.dusk.local');\"",
            "@php artisan migrate --env=testing --ansi",
            "@php artisan bootstrap:application --env=testing --ansi",
            "@php artisan dusk --env=testing"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
